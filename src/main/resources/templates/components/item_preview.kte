@param itemId: Int
@param name: String
@param author: String
@param description: String
@param svg: String

<a class="item-preview" href="/explore/${itemId}">
	<div>
		$unsafe{svg}
	</div>

	<div>
		<h2>${name}</h2>
		<p class="subtext">by ${author}</p>
		<p class="description">${description}</p>
	</div>
</a>